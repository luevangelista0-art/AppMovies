<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/telaInicial">


    <fragment
        android:id="@+id/telaInicial"
        android:name="com.example.appmovies.TelaInicial"
        android:label="fragment_tela_inicial"
        tools:layout="@layout/fragment_tela_inicial" >
        <action
            android:id="@+id/telaInicial_to_emCartaz"
            app:destination="@id/emCartaz"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/emCartaz"
        android:name="com.example.appmovies.EmCartaz"
        android:label="fragment_em_cartaz"
        tools:layout="@layout/fragment_em_cartaz" />
    <fragment
        android:id="@+id/emBreve"
        android:name="com.example.appmovies.EmBreve"
        android:label="fragment_em_breve"
        tools:layout="@layout/fragment_em_breve" />

</navigation>
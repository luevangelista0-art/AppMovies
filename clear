[32m+[m[32m        ViewCompat.setOnApplyWindowInsetsListener(findViewById(R.id.main)) { v, insets ->[m
[32m+[m[32m            val systemBars = insets.getInsets(WindowInsetsCompat.Type.systemBars())[m
[32m+[m[32m            v.setPadding(systemBars.left, systemBars.top, systemBars.right, systemBars.bottom)[m
[32m+[m[32m            insets[m
[32m+[m[32m        }[m
[32m+[m[32m        val filmeId = intent.getIntExtra(chave_id, -1)[m
[32m+[m[32m        val busca = BuscarDetalhes()[m
[32m+[m
[32m+[m[32m        val filmeCompleto = busca.getDetalhe(filmeId)[m
[32m+[m
[32m+[m[32m        if(filmeCompleto != null) {[m
[32m+[m[32m            val img = findViewById<ImageView>(R.id.imgFilme)[m
[32m+[m[32m            val titulo = findViewById<TextView>(R.id.titulo)[m
[32m+[m[32m            val sinopse = findViewById<TextView>(R.id.sinopse)[m
[32m+[m[32m            img.setImageResource(filmeCompleto.img)[m
[32m+[m[32m            titulo.text = filmeCompleto.titulo[m
[32m+[m[32m            sinopse.text = filmeCompleto.sinopse[m
[32m+[m[32m        } else{[m
[32m+[m[32m            val toast = Toast.makeText(this, "Erro ao carregar a p√°gina", Toast.LENGTH_SHORT)[m
[32m+[m[32m            toast.show()[m
[32m+[m[32m            finish()[m
[32m+[m[32m        }[m
[32m+[m
